# Server
SERVER_PORT=8080
GIN_MODE=debug

# PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_USER=mitsume
DB_PASSWORD=mitsume
DB_NAME=mitsume
DB_SSLMODE=disable

# Trino
TRINO_HOST=localhost
TRINO_PORT=8090
TRINO_USER=mitsume
TRINO_CATALOG=memory
TRINO_SCHEMA=default

# JWT
JWT_SECRET=your-secret-key-change-in-production

# Google OAuth (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URL=http://localhost:8080/api/auth/google/callback

# Query Cache (Redis/Valkey)
CACHE_ENABLED=false
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
CACHE_TTL_HIGH_SECONDS=3600
CACHE_TTL_NORMAL_SECONDS=600
CACHE_TTL_LOW_SECONDS=60
CACHE_KEY_PREFIX=mitsume:cache:

# Initial Admin User (optional)
MITSUME_ADMIN_USERNAME=admin
MITSUME_ADMIN_PASSWORD=admin_password
